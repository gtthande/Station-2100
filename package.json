{
    "name": "station-2100",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --port 8080 --strictPort",
        "watchdog": "tsx scripts/watchdog.ts",
        "autopilot": "concurrently \"npm run dev\" \"npm run watchdog\"",
        "build": "tsc; vite build",
        "build:dev": "vite build --mode development",
        "preview": "vite preview",
        "sync:server": "tsx server/sync-server.ts",
        "check:sync": "tsx scripts/check-sync.ts",
        "sync-up": "powershell -ExecutionPolicy Bypass -File sync-up.ps1",
        "git-sync": "powershell -ExecutionPolicy Bypass -File git-sync.ps1",
        "db:push": "npx supabase@latest db push --yes --password \"$env:SUPABASE_DB_PASSWORD\"",
        "link": "npx supabase@latest link --password \"$env:SUPABASE_DB_PASSWORD\"",
        "start:all": "npm run db:push && npm run dev",
        "reset": "taskkill /F /IM node.exe /T 2>$null && npm run link && npm run db:push && npm run dev",
        "fix:stubs": "node scripts/dev-scan-and-stub.mjs",
        "migrate": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"$name = Read-Host 'Migration name (e.g. add_work_orders)'; npx supabase@latest migration new \\\"$name\\\"; Write-Host '*** Edit the generated SQL in supabase/migrations, then press Enter to push...'; Read-Host; npx supabase@latest db push --yes --password \\\"$env:SUPABASE_DB_PASSWORD\\\"\"",
        "dev:clean": "cross-env-shell \"rmdir /s /q node_modules\\.vite 2>nul & npm run dev || rm -rf node_modules/.vite && npm run dev\"",
        "sync:dev-profile": "powershell -ExecutionPolicy Bypass -File scripts/sync-dev-profile.ps1"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.0",
        "@prisma/client": "^6.17.1",
        "@radix-ui/react-accordion": "^1.2.0",
